   /* Employee Temporary Table */
  CREATE TABLE AA_EMP_MASTER_TMP_TBL 
   (	EMPID VARCHAR2(30 CHAR), 
	NATIONALITY VARCHAR2(240 CHAR), 
	ENAME VARCHAR2(2000 CHAR), 
	GENDER VARCHAR2(80 CHAR), 
	AGE NUMBER, 
	MARITAL_STATUS VARCHAR2(80 CHAR), 
	EDUCATION VARCHAR2(80 CHAR), 
	DISABLED_FLG VARCHAR2(30 CHAR), 
	DEPTNO VARCHAR2(30 CHAR), 
	DEPT_NAME VARCHAR2(255 CHAR), 
	ORIG_HIRE_DT DATE, 
	LAST_WORKED_DT DATE, 
	EVENT_DT NUMBER(10,0) NOT NULL ENABLE, 
	TENURE NUMBER, 
	JOB_NAME VARCHAR2(255 CHAR), 
	IS_ACTIVE VARCHAR2(1), 
	ISCHURN CHAR(1)
   );
   
     /* Employee Churn Candidate Table */ 
  CREATE TABLE AA_EMP_MASTER_CHURN_TMP_TBL 
   (	EMPID VARCHAR2(30 CHAR), 
	ISCHURN CHAR(1)
   );
   
     /* Employee Master Table */ 
  CREATE TABLE AA_EMP_MASTER_TBL 
   (	EMPID VARCHAR2(30 CHAR), 
	NATIONALITY VARCHAR2(240 CHAR), 
	ENAME VARCHAR2(2000 CHAR), 
	GENDER VARCHAR2(80 CHAR), 
	AGE NUMBER, 
	MARITAL_STATUS VARCHAR2(80 CHAR), 
	EDUCATION VARCHAR2(80 CHAR), 
	DISABLED_FLG VARCHAR2(30 CHAR), 
	DEPTNO VARCHAR2(30 CHAR), 
	DEPT_NAME VARCHAR2(255 CHAR), 
	ORIG_HIRE_DT DATE, 
	LAST_WORKED_DT DATE, 
	EVENT_DT NUMBER(10,0) NOT NULL ENABLE, 
	TENURE NUMBER, 
	JOB_NAME VARCHAR2(255 CHAR), 
	IS_ACTIVE VARCHAR2(1), 
	ISCHURN CHAR(1), 
	 CONSTRAINT PK_AA_EMP_MASTER_TBL PRIMARY KEY (EMPID)  
   );
  
     /* Employee Compa Ratio Table */
  CREATE TABLE AA_EMP_COMPA_TBL 
   (	EMPID VARCHAR2(30 CHAR), 
	COMPA_Y1 NUMBER, 
	COMPA_Y2 NUMBER, 
	COMPA_Y3 NUMBER, 
	COMPA_Y4 NUMBER, 
	 CONSTRAINT PK_AA_EMP_COMPA_TBL PRIMARY KEY (EMPID)  
   );
   
     /* Employee Department Change Table */ 
  CREATE TABLE AA_EMP_DEPT_CHNG_TBL 
   (	EMPID VARCHAR2(80 CHAR), 
	REASON VARCHAR2(50 CHAR), 
	NO_OF_DEPT_CNG_L4Y NUMBER, 
	 CONSTRAINT PK_AA_EMP_DEPT_CHNG_TBL PRIMARY KEY (EMPID)  
   ) ;
   
     /* Employee Performance Table  */ 
  CREATE TABLE AA_EMP_PERFORMANCE_TBL 
   (	EMPID VARCHAR2(30 CHAR), 
	PR_Y1 NUMBER, 
	PR_Y2 NUMBER, 
	PR_Y3 NUMBER, 
	PR_Y4 NUMBER, 
	PR_Y5 NUMBER, 
	PR_Y6 NUMBER, 
	 CONSTRAINT PK_AA_EMP_PERFORMANCE_TBL PRIMARY KEY (EMPID)
   ) ;
   
     /* Employee Promotion Table */ 
  CREATE TABLE AA_EMP_PROMO_TBL 
   (	EMPID VARCHAR2(30 CHAR), 
	NO_OF_PROMOTION_L4Y NUMBER, 
	 CONSTRAINT PK_AA_EMP_PROMO_TBL PRIMARY KEY (EMPID)
   ) ; 
   
     /* Employee Salary Revision table */ 
  CREATE TABLE AA_EMP_SALREV_TBL 
   (	EMPID VARCHAR2(30 CHAR), 
	NO_OF_SALREV_L4Y NUMBER, 
	 CONSTRAINT PK_AA_EMP_SALREV_TBL PRIMARY KEY (EMPID)
   ) ; 
   
     /* Employee Sick Leave Table */ 
  CREATE TABLE AA_EMP_SICK_LEAVES_TBL 
   (	EMPID VARCHAR2(30 CHAR), 
	SICKLV_Y1 NUMBER, 
	SICKLV_Y2 NUMBER, 
	SICKLV_Y3 NUMBER, 
	SICKLV_Y4 NUMBER, 
	 CONSTRAINT PK_AA_EMP_SICK_LEAVES_TBL PRIMARY KEY (EMPID) 
   ) ; 
   
     /* Employee Supervisor Table */ 
  CREATE TABLE AA_EMP_SUPERVISOR_TBL 
   (	EMPID VARCHAR2(30 CHAR), 
	NO_OF_SVR_CHNG_L4Y NUMBER, 
	 CONSTRAINT PK_AA_EMP_SUPERVISOR_TBL PRIMARY KEY (EMPID)
   ) ;
   
  
   /* Payroll Cost Table */   
  CREATE TABLE AA_PAYROLL_COST_TBL 
   (	YRMON VARCHAR2(50 CHAR), 
	YR NUMBER(4,0), 
	MONTH_NUMBER NUMBER(2,0), 
	PAYROLL_COST NUMBER, 
	 CONSTRAINT PK_AA_PAYROLL_COST PRIMARY KEY (YRMON)
   ) ;
   
   /* Model Training Input Table */
  CREATE TABLE AA_COMMN_MDL_TRAIN_TBL 
   (	EMPID VARCHAR2(30 CHAR), 
	NATIONALITY VARCHAR2(240 CHAR), 
	ENAME VARCHAR2(2000 CHAR), 
	GENDER VARCHAR2(80 CHAR), 
	AGE NUMBER, 
	AGE_BAND CHAR(10), 
	MARITAL_STATUS VARCHAR2(80 CHAR), 
	EDUCATION VARCHAR2(80 CHAR), 
	DISABLED_FLG VARCHAR2(30 CHAR), 
	DEPTNO VARCHAR2(30 CHAR), 
	CURR_DEPT_NAME VARCHAR2(255 CHAR), 
	ORIG_HIRE_DT DATE, 
	LAST_WORKED_DT DATE, 
	TENURE NUMBER, 
	TENURE_BAND VARCHAR2(13), 
	JOB_NAME VARCHAR2(255 CHAR), 
	COMPA_YR1 NUMBER, 
	COMPA_YR2 NUMBER, 
	COMPA_YR3 NUMBER, 
	COMPA_YR4 NUMBER, 
	CHNG_COMPA_L4Y NUMBER, 
	CHNG_COMPA_LY NUMBER, 
	CHNG_DIR_COMPA_L4Y VARCHAR2(9), 
	CHNG_DIR_COMPA_LY VARCHAR2(9), 
	COMPA_BAND_YR1 VARCHAR2(12), 
	COMPA_BAND_YR2 VARCHAR2(12), 
	COMPA_BAND_YR3 VARCHAR2(12), 
	COMPA_BAND_YR4 VARCHAR2(12), 
	PERF_YR1 NUMBER, 
	PERF_YR2 NUMBER, 
	PERF_YR3 NUMBER, 
	PERF_YR4 NUMBER, 
	AVG_PERF_L4Y NUMBER, 
	WT_AVG_PERF_L4Y NUMBER, 
	PERF_BAND_YR1 VARCHAR2(43), 
	PERF_BAND_YR2 VARCHAR2(43), 
	PERF_BAND_YR3 VARCHAR2(43), 
	PERF_BAND_YR4 VARCHAR2(43), 
	SICKLV_YR1 NUMBER, 
	SICKLV_YR2 NUMBER, 
	SICKLV_YR3 NUMBER, 
	SICKLV_YR4 NUMBER, 
	AVG_SICKLV_L4Y NUMBER, 
	CHNG_SICKLV_L4Y NUMBER, 
	CHNG_SICKLV_LY NUMBER, 
	CHNG_DIR_SICKLV_L4Y VARCHAR2(9), 
	CHNG_DIR_SICKLV_LY VARCHAR2(9), 
	DEPT_CHNG_L4Y NUMBER, 
	PROMO_L4Y NUMBER, 
	SALREV_L4Y NUMBER, 
	SVR_CHNG_L4Y NUMBER, 
	ISCHURN CHAR(1), 
	PERF_CATEG1 VARCHAR2(10), 
	PERF_CATEG2 VARCHAR2(17), 
	PERF_CATEG3 VARCHAR2(14), 
	 CONSTRAINT PK_AA_COMMN_MDL_TRAIN_TBL PRIMARY KEY (EMPID)
   ) ;
   
